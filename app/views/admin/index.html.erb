<h1>Admin Dashboard</h1>

<h3>Users:</h3>
<ul>
<% @users.each do | user| %>
    <li><%= user.full_name %> | <%= user.email %> | <%= link_to "Delete User", user, data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} %>
</li>
<% end %>
</ul>

<%= link_to "Log out", logout_path, data: {turbo_method: :delete} %>
