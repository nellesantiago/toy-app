<h1>Feed</h1>

<% @posts.each do | post | %>
    <li>
    <%= post.user.full_name %><br>
    <%= post.title %><br>
    <%= post.description %><br><br>
    </li>
<% end %>


<% if current_user.admin?%>
<%= link_to "All Users", users_path %>
<%= link_to "Log out", logout_path, data: { turbo_method: :delete } %>
<% end %>
